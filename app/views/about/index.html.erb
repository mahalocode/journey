      <div class="box">
        <pre><code class="language-ruby">
          puts &quot;Enter a number: &quot;
          user_input = gets.chomp
          converted_input = user_input.to_i
          first_digit = converted_input % 10
          suffix = nil
          if converted_input &gt;= 10 &amp;&amp; converted_input &lt;= 20
          suffix = &quot;th&quot;
          elsif first_digit == 1
          suffix = &quot;st&quot;
          elsif first_digit == 2
          suffix = &quot;nd&quot;
          elsif first_digit == 3
          suffix = &quot;rd&quot;
          else suffix = &quot;th&quot;
          end
          puts &quot;You are in the #{converted_input}#{suffix} place.&quot;
        </code></pre>
